{
  "name": "@alloy-di/example-library-external",
  "version": "0.0.1",
  "description": "Example external library demonstrating Alloy DI integration via providers",
  "private": true,
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "rimraf dist && rolldown -c",
    "dev": "rolldown -c rolldown.config.ts --watch",
    "format": "prettier -u --write ./",
    "format:check": "prettier -u --check ./",
    "lint": "oxlint --fix --type-aware --config ./.oxlintrc.json .",
    "lint:check": "oxlint --type-aware --config ./.oxlintrc.json .",
    "test:cover": "echo No tests",
    "typecheck": "tsc -b . --noEmit"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./logger": {
      "types": "./dist/logger.d.ts",
      "import": "./dist/logger.js"
    },
    "./console-output": {
      "types": "./dist/console-output.d.ts",
      "import": "./dist/console-output.js"
    }
  },
  "keywords": [
    "logger",
    "di",
    "dependency-injection",
    "example"
  ],
  "author": "",
  "license": "UNLICENSED",
  "packageManager": "pnpm@10.22.0",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "rimraf": "^6.1.0",
    "rolldown": "1.0.0-beta.52",
    "rolldown-plugin-dts": "^0.18.1"
  },
  "dependencies": {
    "tslib": "^2.8.1"
  }
}
